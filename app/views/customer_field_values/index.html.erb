<p id="notice"><%= notice %></p>

<h1>Listing Customer Field Values</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Customer field</th>
      <th>Field value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customer_field_values.each do |customer_field_value| %>
      <tr>
        <td><%= customer_field_value.customer_id %></td>
        <td><%= customer_field_value.customer_field_id %></td>
        <td><%= customer_field_value.field_value %></td>
        <td><%= link_to 'Show', customer_field_value %></td>
        <td><%= link_to 'Edit', edit_customer_field_value_path(customer_field_value) %></td>
        <td><%= link_to 'Destroy', customer_field_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customer field value', new_customer_field_value_path %>
